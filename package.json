{
  "name": "@hubspot/prettier-plugin-hubl",
  "version": "0.3.0",
  "main": "prettier-plugin-hubl.js",
  "private": true,
  "dependencies": {},
  "peerDependencies": {
    "prettier": "3.x"
  },
  "devDependencies": {
    "lerna": "^8.1.2",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "yarn watch",
    "watch": "lerna exec --parallel yarn watch",
    "run:prettier": "lerna --scope='prettier-printer-hubl' run",
    "run:parser": "lerna --scope='hubl-parser' run",
    "watch:prettier": "yarn run:prettier watch",
    "watch:parser": "yarn run:parser watch",
    "build:prettier": "yarn run:prettier build",
    "build:parser": "yarn run:parser build",
    "prepare": "(npm run build:parser) && (npm run build:prettier)",
    "npm:publish-beta": "npm publish . --tag beta",
    "npm:publish-latest": "npm publish . --tag latest"
  },
  "files": [
    "parser/dist",
    "prettier/dist"
  ],
  "workspaces": [
    "parser",
    "prettier"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:HubSpot/prettier-plugin-hubl.git"
  },
  "author": "HubSpot, Inc.",
  "license": "Apache-2.0"
}
