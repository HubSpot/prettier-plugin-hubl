{
  "name": "prettier-printer-hubl",
  "version": "1.1.0",
  "description": "",
  "type": "module",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "start": "nodemon --config ./configuration/nodemon.inline.json",
    "test": "node --experimental-vm-modules ../node_modules/jest/bin/jest.js -c ./tests/configuration/jest.config.ts",
    "watch": "nodemon",
    "update-test": "tsc && jest -c ./configuration/jest.config.js --update-snapshot",
    "test-inline": "tsc && node ./src/test-inline.ts"
  },
  "author": "HubSpot, Inc.",
  "license": "Apache-2.0",
  "dependencies": {
    "@prettier/sync": "^0.5.2",
    "hubl-parser": "1.0.0",
    "prettier": "^3.2.5"
  },
  "devDependencies": {
    "@types/node": "^18.14.2",
    "@types/prettier": "^3.0.0",
    "jest": "^29.0.0",
    "jest-watch-typeahead": "^0.2.1",
    "nodemon": "^2.0.13",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "private": true,
  "nodemonConfig": {
    "watch": [
      "tests",
      "src"
    ],
    "exec": "tsc && yarn test",
    "ext": "js, json, ts, proto, html",
    "ignoreRoot": [
      ".git"
    ]
  }
}
