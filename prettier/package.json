    "prettier": "^2.4.1"
  },
  "devDependencies": {
    "@types/node": "^16.10.2",
    "@types/prettier": "^2.4.1",
    "foreman": "^3.0.1",
    "jest": "^27.0.0",
    "jest-watch-typeahead": "^0.2.1",
    "nodemon": "^2.0.13",
    "ts-node": "^9.1.1",
    "typescript": "^4.6.3"
  },
  "private": true
